<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hedian.mapper.SysGroupMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hedian.entity.SysGroup">
        <id column="grp_id" property="grpId" />
        <result column="grp_type" property="grpType" />
        <result column="grp_name" property="grpName" />
        <result column="grp_desc" property="grpDesc" />
        <result column="dept_id" property="deptId" />
        <result column="user_id_create" property="userIdCreate" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
        <result column="useflag" property="useflag" />
        <result column="user_id_mod" property="userIdMod" />
        <result column="delflag" property="delflag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        grp_id AS grpId, grp_type AS grpType, grp_name AS grpName, grp_desc AS grpDesc,dept_id AS deptId, user_id_create AS userIdCreate, gmt_create AS gmtCreate, gmt_modified AS gmtModified, useflag, user_id_mod AS userIdMod, delflag
    </sql>

</mapper>
